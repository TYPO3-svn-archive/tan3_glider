<T3DataStructure>
  <meta>
    <langDisable>1</langDisable>
  </meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
				<sheetTitle>LLL:EXT:tan3_glider/locallang.xml:flexform.sheets.general.title</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
          			<!-- Content Elements browser = Pick your content elements -->
					<contentelements>
						<TCEforms>
							<label>LLL:EXT:tan3_glider/locallang.xml:flexform.sheets.general.contentElements</label>
							<config>
								<type>group</type>
				                <internal_type>db</internal_type>
				                <allowed>tt_content</allowed>
				                <size>10</size>
				                <maxitems>22</maxitems>
				                <minitems>0</minitems>
				                <show_thumbs>1</show_thumbs>
							</config>
						</TCEforms>					
          			</contentelements>
						
  					<description>
		            <TCEforms>
		              <label>LLL:EXT:tan3_glider/locallang.xml:flexform.sheets.general.description</label>
		              <config>
		                <type>text</type>
		                <cols>40</cols>
		                <rows>10</rows>
		              </config>
		            </TCEforms>
		          </description>
					
					<width>
						<TCEforms>
							<label>LLL:EXT:tan3_glider/locallang.xml:flexform.sheets.general.width</label>
							<config>
								<type>input</type>
								<size>4</size>
							</config>
						</TCEforms>
					</width>
					
					<height>
						<TCEforms>
							<label>LLL:EXT:tan3_glider/locallang.xml:flexform.sheets.general.height</label>
							<config>
								<type>input</type>
								<size>4</size>
							</config>
						</TCEforms>
					</height>
					<tabs>
		            <TCEforms>
		              <label>LLL:EXT:tan3_glider/locallang.xml:flexform.sheets.general.tabs</label>
		              <config>
		                <type>check</type>
		                <default>1</default>
		              </config>
		            </TCEforms>
		          </tabs>
		
					<arrows>
		            <TCEforms>
		              <label>LLL:EXT:tan3_glider/locallang.xml:flexform.sheets.general.arrows</label>
		              <config>
		                <type>check</type>
		                <default>1</default>
		              </config>
		            </TCEforms>
		          </arrows>
	    </el>
	  </ROOT>
    </sDEF>
  </sheets>
</T3DataStructure>
